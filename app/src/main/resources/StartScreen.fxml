<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="ch.zhaw.pm2.ironbelchers.ui.StartScreenController">
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="10.0"
         AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ironbelchers">
         <font>
            <Font size="40.0"/>
         </font>
      </Text>
      <Button id="playBtn" mnemonicParsing="false" onAction="#playPressed" text="Play"/>
      <Button id="quitBtn" mnemonicParsing="false" onAction="#quitPressed" text="Quit"/>
      <Text id="errorText" fx:id="errorText" strokeType="OUTSIDE" strokeWidth="0.0" text="Error message"
            textAlignment="CENTER" visible="false"/>
   </VBox>
</AnchorPane>
